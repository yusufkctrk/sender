<Home_Screen>:
    name: "home"
    id:"deneme"
    BoxLayout:
        orientation:'vertical'

        MDToolbar:
            title: app.APP_NAME
            size_hint: 1,.08
            elevation: 5
        MDBottomNavigation:
            id: android_tabs
            MDBottomNavigationItem:
                name: "chat_scr"
                text: f"CHATS"
                icon: "chat"
                ScrollView:
                    MDList:
                        id: chat_tab
            MDBottomNavigationItem:
                name: "search_scr"
                text: f"USERS"
                icon: "account"
                MDBoxLayout:
                    orientation: 'vertical'
                    spacing: dp(5)
                    padding: dp(10)
                    ScrollView:
                        MDList:
                            id: search_items